  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.8",
    "extract-text-webpack-plugin": "^3.0.2",
    "happypack": "^4.0.0",
    "html-webpack-plugin": "^2.30.1",
    "style-loader": "^0.19.1",
    "vue": "^2.5.13",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.0"
  }
