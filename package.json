{
  "name": "webpack-bricks",
  "version": "0.1.35",
  "main": "index.js",
  "scripts": {
    "dev:webpack": "cd examples && rm -rf dist && webpack -w --progress",
    "dev": "nodemon -w webpack.config.js --exec \"npm run dev:webpack\"",
